@model IEnumerable<WebAppTest.Models.UsuariosModel>



<h1>Crear Usuario</h1>

<form method="post">
    <label for="name">Nombre:</label>
    <input type="text" id="nombre" name="nombre" />

    <label for="apellido">Apellido:</label>
    <input type="text" id="apellido" name="apellido" />

    <label for="email">Correo Electrónico:</label>
    <input type="email" id="email" name="email" />

    <label for="fecha">Fecha de Nacimiento:</label>
    <input type="date" id="fecha" name="fecha" />

    <label for="telefono">Teléfono:</label>
    <input type="number" id="telefono" name="telefono" />

    <label for="pais">País de Residencia:</label>
    <input class="dropdown" type="text" id="pais" name="pais" />

    <label for="contacto">
        ¿Desea recibir información?:
    </label>
    <input type="checkbox" id="contacto" name="contacto" />

    <button asp-controller="Usuarios" asp-action="AddUsuario" type="submit">Grabar</button>
</form>
